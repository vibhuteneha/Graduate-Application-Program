    );

    create table degrees (
        id int4 not null,
        degree_earned varchar(255),
        major varchar(255),
        school_name varchar(255),
        time_period varchar(255),
        application_id int4,
        primary key (id)
    );

    create table departments (
        id int4 not null,
        department_name varchar(255),
